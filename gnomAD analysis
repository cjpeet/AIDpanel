Finnish.Sum <-c(ADA2.sum,CARD14.sum,IL1RN.sum,IL36RN.sum,LPIN2.sum,MEFV.sum,MVK.sum,NLRC4.sum,NLRP3.sum,NLRP12.sum,NOD2.sum,PLCG2.sum,PSMB4.sum,PSMB8.sum,PSMB9.sum,PSTPIP1.sum,SH3BP2.sum,TMEM173.sum,TNFAIP3.sum,TNFRSF1A.sum,OTULIN.sum, RBCK1.sum, SLC29A3.sum, UBA1.sum)
Finnish.Max <-c(ADA2.max,CARD14.max,IL1RN.max,IL36RN.max,LPIN2.max,MEFV.max,MVK.max,NLRC4.max,NLRP3.max,NLRP12.max,NOD2.max,PLCG2.max,PSMB4.max,PSMB8.max,PSMB9.max,PSTPIP1.max,SH3BP2.max,TMEM173.max,TNFAIP3.max,TNFRSF1A.max, OTULIN.max, RBCK1.max, SLC29A3.max, UBA1.max)
Finnish.CF <-c(ADA2.CF,CARD14.CF,IL1RN.CF,IL36RN.CF,LPIN2.CF,MEFV.CF,MVK.CF,NLRC4.CF,NLRP3.CF,NLRP12.CF,NOD2.CF,PLCG2.CF,PSMB4.CF,PSMB8.CF,PSMB9.CF,PSTPIP1.CF,SH3BP2.CF,TMEM173.CF,TNFAIP3.CF,TNFRSF1A.CF, OTULIN.CF, RBCK1.CF, SLC29A3.CF, UBA1.CF) 
Finnish.table <- data.frame(Gene,Finnish.Sum,Finnish.Max,Finnish.CF)

