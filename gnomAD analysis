Other.Sum <-c(ADA2.sum,CARD14.sum,IL1RN.sum,IL36RN.sum,LPIN2.sum,MEFV.sum,MVK.sum,NLRC4.sum,NLRP3.sum,NLRP12.sum,NOD2.sum,PLCG2.sum,PSMB4.sum,PSMB8.sum,PSMB9.sum,PSTPIP1.sum,SH3BP2.sum,TMEM173.sum,TNFAIP3.sum,TNFRSF1A.sum,OTULIN.sum, RBCK1.sum, SLC29A3.sum, UBA1.sum)
Other.Max <-c(ADA2.max,CARD14.max,IL1RN.max,IL36RN.max,LPIN2.max,MEFV.max,MVK.max,NLRC4.max,NLRP3.max,NLRP12.max,NOD2.max,PLCG2.max,PSMB4.max,PSMB8.max,PSMB9.max,PSTPIP1.max,SH3BP2.max,TMEM173.max,TNFAIP3.max,TNFRSF1A.max, OTULIN.max, RBCK1.max, SLC29A3.max, UBA1.max)
Other.CF <-c(ADA2.CF,CARD14.CF,IL1RN.CF,IL36RN.CF,LPIN2.CF,MEFV.CF,MVK.CF,NLRC4.CF,NLRP3.CF,NLRP12.CF,NOD2.CF,PLCG2.CF,PSMB4.CF,PSMB8.CF,PSMB9.CF,PSTPIP1.CF,SH3BP2.CF,TMEM173.CF,TNFAIP3.CF,TNFRSF1A.CF, OTULIN.CF, RBCK1.CF, SLC29A3.CF, UBA1.CF) 
Other.table <- data.frame(Gene,Other.Sum,Other.Max,Other.CF)

##Construct all ethnicity table


#Split data frame by genes and assign new dataframes to objects
X <- split(All, All$Gene)
ADA2 <- X$ADA2
CARD14 <- X$CARD14
IL1RN <- X$IL1RN
IL36RN <- X$IL36RN
LPIN2 <- X$LPIN2
MEFV <- X$MEFV
MVK <- X$MVK
NLRC4 <- X$NLRC4
NLRP3 <- X$NLRP3
NLRP12 <- X$NLRP12
NOD2 <- X$NOD2
PLCG2 <- X$PLCG2
PSMB4 <- X$PSMB4
PSMB8 <- X$PSMB8
PSMB9 <- X$PSMB9
PSTPIP1 <- X$PSTPIP1
SH3BP2 <- X$SH3BP2
TMEM173 <- X$TMEM173
TNFAIP3 <- X$TNFAIP3
TNFRSF1A <- X$TNFRSF1A
OTULIN <- X$OTULIN
RBCK1 <- X$RBCK1
SLC29A3 <- X$SLC29A3
UBA1 <- X$UBA1

#Combined frequency calculations
ADA2.sum <- sum(ADA2$Allele.Count)
CARD14.sum <- sum(CARD14$Allele.Count)
IL1RN.sum <- sum(IL1RN$Allele.Count)
IL36RN.sum <- sum(IL36RN$Allele.Count)
LPIN2.sum <- sum(LPIN2$Allele.Count)
MEFV.sum <- sum(MEFV$Allele.Count)
MVK.sum <- sum(MVK$Allele.Count)
NLRC4.sum <- sum(NLRC4$Allele.Count)
NLRP3.sum <- sum(NLRP3$Allele.Count)
NLRP12.sum <- sum(NLRP12$Allele.Count)
NOD2.sum <- sum(NOD2$Allele.Count)
PLCG2.sum <- sum(PLCG2$Allele.Count)
PSMB4.sum <- sum(PSMB4$Allele.Count)
PSMB8.sum <- sum(PSMB8$Allele.Count)
PSMB9.sum <- sum(PSMB9$Allele.Count)
PSTPIP1.sum <- sum(PSTPIP1$Allele.Count)
SH3BP2.sum <- sum(SH3BP2$Allele.Count)
TMEM173.sum <- sum(TMEM173$Allele.Count)
TNFAIP3.sum <- sum(TNFAIP3$Allele.Count)
TNFRSF1A.sum <- sum(TNFRSF1A$Allele.Count)
OTULIN.sum <- sum(OTULIN$Allele.Count)
RBCK1.sum <- sum(RBCK1$Allele.Count)
SLC29A3.sum <- sum(SLC29A3$Allele.Count)
UBA1.sum <- sum(UBA1$Allele.Count)

ADA2.max <- max(ADA2$Allele.Number)
CARD14.max <- max(CARD14$Allele.Number)
IL1RN.max <- max(IL1RN$Allele.Number)
IL36RN.max <- max(IL36RN$Allele.Number)
LPIN2.max <- max(LPIN2$Allele.Number)
MEFV.max <- max(MEFV$Allele.Number)
MVK.max <- max(MVK$Allele.Number)
NLRC4.max <- max(NLRC4$Allele.Number)
NLRP3.max <- max(NLRP3$Allele.Number)
NLRP12.max <- max(NLRP12$Allele.Number)
NOD2.max <- max(NOD2$Allele.Number)
PLCG2.max <- max(PLCG2$Allele.Number)
PSMB4.max <- max(PSMB4$Allele.Number)
PSMB8.max <- max(PSMB8$Allele.Number)
PSMB9.max <- max(PSMB9$Allele.Number)
PSTPIP1.max <- max(PSTPIP1$Allele.Number)
SH3BP2.max <- max(SH3BP2$Allele.Number)
TMEM173.max <- max(TMEM173$Allele.Number)
TNFAIP3.max <- max(TNFAIP3$Allele.Number)
TNFRSF1A.max <- max(TNFRSF1A$Allele.Number)
OTULIN.max <- max(OTULIN$Allele.Number)
RBCK1.max <- max(RBCK1$Allele.Number)
SLC29A3.max <- max(SLC29A3$Allele.Number)
UBA1.max <- max(UBA1$Allele.Number)

ADA2.CF <- ADA2.sum/ADA2.max 
CARD14.CF <- CARD14.sum/CARD14.max
IL1RN.CF <- IL1RN.sum/IL1RN.max
IL36RN.CF <- IL36RN.sum/IL36RN.max
LPIN2.CF <- LPIN2.sum/LPIN2.max
MEFV.CF <- MEFV.sum/MEFV.max 
MVK.CF <- MVK.sum/MVK.max
NLRC4.CF <- NLRC4.sum/NLRC4.max
NLRP3.CF <- NLRP3.sum/NLRP3.max
NLRP12.CF <- NLRP12.sum/NLRP12.max
NOD2.CF <- NOD2.sum/NOD2.max
PLCG2.CF <- PLCG2.sum/PLCG2.max
PSMB4.CF <- PSMB4.sum/PSMB4.max
PSMB8.CF <- PSMB8.sum/PSMB8.max
PSMB9.CF <- PSMB9.sum/PSMB9.max
PSTPIP1.CF <- PSTPIP1.sum/PSTPIP1.max
SH3BP2.CF <- SH3BP2.sum/SH3BP2.max
TMEM173.CF <- TMEM173.sum/TMEM173.max
TNFAIP3.CF <- TNFAIP3.sum/TNFAIP3.max
TNFRSF1A.CF <- TNFRSF1A.sum/TNFRSF1A.max
OTULIN.CF <- OTULIN.sum/OTULIN.max
RBCK1.CF <- RBCK1.sum/RBCK1.max
SLC29A3.CF <- SLC29A3.sum/SLC29A3.max
UBA1.CF <- UBA1.sum/UBA1.max

#Construct data frame
Gene <- c('ADA2','CARD14','IL1RN','IL36RN','LPIN2','MEFV','MVK','NLRC4','NLRP3','NLRP12','NOD2','PLCG2','PSMB4','PSMB8','PSMB9','PSTPIP1','SH3BP2','TMEM173','TNFAIP3','TNFRSF1A', 'OTULIN','RBCK1','SLC29A3','UBA1')
All.Sum <-c(ADA2.sum,CARD14.sum,IL1RN.sum,IL36RN.sum,LPIN2.sum,MEFV.sum,MVK.sum,NLRC4.sum,NLRP3.sum,NLRP12.sum,NOD2.sum,PLCG2.sum,PSMB4.sum,PSMB8.sum,PSMB9.sum,PSTPIP1.sum,SH3BP2.sum,TMEM173.sum,TNFAIP3.sum,TNFRSF1A.sum,OTULIN.sum, RBCK1.sum, SLC29A3.sum, UBA1.sum)
All.Max <-c(ADA2.max,CARD14.max,IL1RN.max,IL36RN.max,LPIN2.max,MEFV.max,MVK.max,NLRC4.max,NLRP3.max,NLRP12.max,NOD2.max,PLCG2.max,PSMB4.max,PSMB8.max,PSMB9.max,PSTPIP1.max,SH3BP2.max,TMEM173.max,TNFAIP3.max,TNFRSF1A.max, OTULIN.max, RBCK1.max, SLC29A3.max, UBA1.max)
All.CF <-c(ADA2.CF,CARD14.CF,IL1RN.CF,IL36RN.CF,LPIN2.CF,MEFV.CF,MVK.CF,NLRC4.CF,NLRP3.CF,NLRP12.CF,NOD2.CF,PLCG2.CF,PSMB4.CF,PSMB8.CF,PSMB9.CF,PSTPIP1.CF,SH3BP2.CF,TMEM173.CF,TNFAIP3.CF,TNFRSF1A.CF, OTULIN.CF, RBCK1.CF, SLC29A3.CF, UBA1.CF) 
All.table <- data.frame(Gene,All.Sum,All.Max,All.CF)

combined <- Reduce(function(x,y) merge(x = x, y = y, by = "Gene"), list(All.table, African.table, Latino.table,Jewish.table,East.Asian.table,Finnish.table,NFE.table,South.Asian.table,Other.table))
write.csv(combined,"{desiredfilenameanddirectory}")
