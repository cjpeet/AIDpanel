TMEM173 <- X$TMEM173
TNFAIP3 <- X$TNFAIP3
TNFRSF1A <- X$TNFRSF1A
OTULIN <- X$OTULIN
RBCK1 <- X$RBCK1
SLC29A3 <- X$SLC29A3
UBA1 <- X$UBA1

