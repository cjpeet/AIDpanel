#Download gnomad CSV file for ADA2 and ensure saved to chosen directory with name \{genename}.MAF.csv
#Open R
#Load dplyr package
library(dplyr)

#Open MAFs
ADA2.MAF <- read.csv("ADA2.MAF.csv", header = TRUE)
IL1RN.MAF <- read.csv("IL1RN.MAF.csv", header = TRUE)
IL36RN.MAF <- read.csv("IL36RN.MAF.csv", header = TRUE)
CARD14.MAF <- read.csv("CARD14.MAF.csv", header = TRUE)
LPIN2.MAF <- read.csv("LPIN2.MAF.csv", header = TRUE)
MEFV.MAF <- read.csv("MEFV.MAF.csv", header = TRUE)
MVK.MAF <- read.csv("MVK.MAF.csv", header = TRUE)
NLRC4.MAF <- read.csv("NLRC4.MAF.csv", header = TRUE)
NLRP3.MAF <- read.csv("NLRP3.MAF.csv", header = TRUE)
NLRP12.MAF <- read.csv("NLRP12.MAF.csv", header = TRUE)
NOD2.MAF <- read.csv("NOD2.MAF.csv", header = TRUE)
PLCG2.MAF <- read.csv("PLCG2.MAF.csv", header = TRUE)
PSTPIP1.MAF <- read.csv("PSTPIP1.MAF.csv", header = TRUE)
PSMB4.MAF <- read.csv("PSMB4.MAF.csv", header = TRUE)
PSMB8.MAF <- read.csv("PSMB8.MAF.csv", header = TRUE)
PSMB9.MAF <- read.csv("PSMB9.MAF.csv", header = TRUE)
SH3BP2.MAF <- read.csv("SH3BP2.MAF.csv", header = TRUE)
TMEM173.MAF <- read.csv("TMEM173.MAF.csv", header = TRUE)
TNFAIP3.MAF <- read.csv("TNFAIP3.MAF.csv", header = TRUE)
TNFRSF1A.MAF <- read.csv("TNFRSF1A.MAF.csv", header = TRUE)
OTULIN.MAF <- read.csv("OTULIN.MAF.csv", header = TRUE)
RBCK1.MAF <- read.csv("RBCK1.MAF.csv", header = TRUE)
SLC29A3.MAF <- read.csv("SLC29A3.MAF.csv", header = TRUE)
UBA1.MAF <- read.csv("UBA1.MAF.csv", header = TRUE)

ADA2.MAF$Gene <- c('ADA2')
CARD14.MAF$Gene <- c('CARD14')
IL1RN.MAF$Gene <- c('IL1RN')
IL36RN.MAF$Gene <- c('IL36RN')
LPIN2.MAF$Gene <- c('LPIN2')
MEFV.MAF$Gene <- c('MEFV')
MVK.MAF$Gene <- c('MVK')
NLRC4.MAF$Gene <- c('NLRC4')
NLRP3.MAF$Gene <- c('NLRP3')
NLRP12.MAF$Gene <- c('NLRP12')
NOD2.MAF$Gene <- c('NOD2')
PLCG2.MAF$Gene <- c('PLCG2')
PSMB4.MAF$Gene <- c('PSMB4')
PSMB8.MAF$Gene <- c('PSMB8')
PSMB9.MAF$Gene <- c('PSMB9')
PSTPIP1.MAF$Gene <- c('PSTPIP1')
SH3BP2.MAF$Gene <- c('SH3BP2')
TMEM173.MAF$Gene <- c('TMEM173')
TNFAIP3.MAF$Gene <- c('TNFAIP3')
TNFRSF1A.MAF$Gene <- c('TNFRSF1A')
OTULIN.MAF$Gene <- c('OTULIN')
RBCK1.MAF$Gene <- c('RBCK1')
SLC29A3.MAF$Gene <- c('SLC29A3')
UBA1.MAF$Gene <- c('UBA1')

#Merge into single dataframe
MAF <- rbind(ADA2.MAF,CARD14.MAF,IL1RN.MAF,IL36RN.MAF,LPIN2.MAF,MEFV.MAF,MVK.MAF,NLRC4.MAF, NLRP3.MAF, NLRP12.MAF, NOD2.MAF, PLCG2.MAF, PSMB4.MAF, PSMB8.MAF, PSMB9.MAF, PSTPIP1.MAF, SH3BP2.MAF, TMEM173.MAF, TNFAIP3.MAF, TNFRSF1A.MAF, OTULIN.MAF, RBCK1.MAF, SLC29A3.MAF,UBA1.MAF)

#Create vector with MAF column names
MAF.col <- c('Chromosome','Position','rsIDs','Reference','Alternate','Source','Filters - exomes','Filters - genomes','Transcript','HGVS Consequence','Protein Consequence','Transcript Consequence','VEP Annotation','ClinVar Clinical Significance','ClinVar Variation ID','Flags','Allele Count','Allele Number','Allele Frequency','Homozygote Count','Hemizygote Count','Allele Count African/African American','Allele Number African/African American','Homozygote Count African/African American','Hemizygote Count African/African American','Allele Count Latino/Admixed American','Allele Number Latino/Admixed American','Homozygote Count Latino/Admixed American','Hemizygote Count Latino/Admixed American','Allele Count Ashkenazi Jewish','Allele Number Ashkenazi Jewish','Homozygote Count Ashkenazi Jewish','Hemizygote Count Ashkenazi Jewish','Allele Count East Asian','Allele Number East Asian','Homozygote Count East Asian','Hemizygote Count East Asian','Allele Count European (Finnish)','Allele Number European (Finnish)','Homozygote Count European (Finnish)','Hemizygote Count European (Finnish)','Allele Count European (non-Finnish)','Allele Number European (non-Finnish)','Homozygote Count European (non-Finnish)','Hemizygote Count European (non-Finnish)','Allele Count Other','Allele Number Other','Homozygote Count Other','Hemizygote Count Other','Allele Count South Asian','Allele Number South Asian','Homozygote Count South Asian','Hemizygote Count South Asian')

#Filter for non synonymous and splice site variants
MAF.coding <- MAF %>% filter(VEP.Annotation != '3_prime_UTR_variant' & VEP.Annotation != '5_prime_UTR_variant' & VEP.Annotation != 'intron_variant' & VEP.Annotation != 'synonymous_variant')

#Create ethnicity specific allele frequency columns
MAF.coding$Allele.Frequency.African.African.American <- (MAF.coding$Allele.Count.African.African.American/MAF.coding$Allele.Number.African.African.American)
MAF.coding$Allele.Frequency.Latino.Admixed.American <- (MAF.coding$Allele.Count.Latino.Admixed.American/MAF.coding$Allele.Number.Latino.Admixed.American)
MAF.coding$Allele.Frequency.Ashkenazi.Jewish <- (MAF.coding$Allele.Count.Ashkenazi.Jewish/MAF.coding$Allele.Number.Ashkenazi.Jewish)
MAF.coding$Allele.Frequency.East.Asian <- (MAF.coding$Allele.Count.East.Asian/MAF.coding$Allele.Number.East.Asian)
MAF.coding$Allele.Frequency.European..Finnish. <- (MAF.coding$Allele.Count.European..Finnish./MAF.coding$Allele.Number.European..Finnish.)
MAF.coding$Allele.Frequency.European..non.Finnish. <- (MAF.coding$Allele.Count.European..non.Finnish./MAF.coding$Allele.Number.European..non.Finnish.)
MAF.coding$Allele.Frequency.Other <- (MAF.coding$Allele.Count.Other/MAF.coding$Allele.Number.Other)
MAF.coding$Allele.Frequency.South.Asian <- (MAF.coding$Allele.Count.South.Asian/MAF.coding$Allele.Number.South.Asian)

#Create filtered data frames with ancestry specific rare variants
All <- MAF.coding %>% filter(Allele.Frequency <= 0.01)
African <- MAF.coding %>% filter(Allele.Frequency.African.African.American <= 0.01)
Latino <- MAF.coding %>% filter(Allele.Frequency.Latino.Admixed.American <= 0.01)
Jewish <- MAF.coding %>% filter(Allele.Frequency.Ashkenazi.Jewish <= 0.01)
East.Asian <- MAF.coding %>% filter(Allele.Frequency.East.Asian <= 0.01)
Finnish <- MAF.coding %>% filter(Allele.Frequency.European..Finnish. <= 0.01)
NFE <- MAF.coding %>% filter(Allele.Frequency.European..non.Finnish. <= 0.01)
Other <- MAF.coding %>% filter(Allele.Frequency.Other <= 0.01)
South.Asian <- MAF.coding %>% filter(Allele.Frequency.South.Asian <= 0.01)

##Construct ancestry specific tables, starting with African ancestry table
#Split data frame by genes and assign new dataframes to objects
X <- split(African, African$Gene)
ADA2 <- X$ADA2
CARD14 <- X$CARD14
IL1RN <- X$IL1RN
IL36RN <- X$IL36RN
LPIN2 <- X$LPIN2
MEFV <- X$MEFV
MVK <- X$MVK
NLRC4 <- X$NLRC4
NLRP3 <- X$NLRP3
